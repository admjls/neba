<div id="nav-wrapper">
    <!-- Nav -->
    <nav id="nav">
        <ul>
            {% for item in site.mainNavItems %}
                <li{%if page.url == item.path%} class="active"{% endif %}><a href="{{item.path}}" title="{{item.title}}">{{item.label}}</a></li>
            {% endfor %}
        </ul>
    </nav>
</div>