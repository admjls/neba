<div id="nav-wrapper" class="old">
    <!-- Nav -->
    <nav id="nav">
        <ul>
            <li class="home"><a href="/index.html">Home</a></li>
            {% for item in site.oldNavItems %}
            <li{% if page.url == item.link %} class="active"{% endif %}>
                <a href="{{item.link}}" title="{{item.title}}">{{item.label}}</a>
            </li>
            {% endfor %}
        </ul>
    </nav>
</div>